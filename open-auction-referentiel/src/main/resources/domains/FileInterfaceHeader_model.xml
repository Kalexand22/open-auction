<?xml version="1.0"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">
  <module name="openauction" package="com.axelor.apps.openauction.db"/>
  <entity name="FileInterfaceHeader">
    <string name="no" title="N°"/>
    <string name="interfaceType" title="Type interface"
      selection="openauction.interfaceType.select"/>
    <string name="processType" title="Type de process"
      selection="openauction.processType.select"/>
    <integer name="fileStartingLine" title="Ligne début fichier"/>
    <string name="fileFormat" title="Format fichier"
      selection="openauction.fileFormat.select"/>
    <string name="fieldSeparator" title="Séparateur"/>
    <string name="fieldDelimiter" title="Délimiteur de champ"/>
    <string name="description" title="Désignation"/>
    <boolean name="exportDescriptionLine" title="Exporter ligne de désignation"/>
  </entity>
</domain-models>