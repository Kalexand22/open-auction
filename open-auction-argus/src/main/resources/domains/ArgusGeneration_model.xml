<?xml version="1.0"?>
<domain-models xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://axelor.com/xml/ns/domain-models">
  <module name="openauction" package="com.axelor.apps.openauction.db"/>
  <entity name="ArgusGeneration">
    <many-to-one name="makeCode" title="Code marque"
      ref="com.axelor.apps.openauction.db.ArgusMake"/>
    <many-to-one name="productCode" title="Code produit"
      ref="com.axelor.apps.openauction.db.ArgusProduct"/>
    <many-to-one name="familyCode" title="Code famille"
      ref="com.axelor.apps.openauction.db.ArgusFamily"/>
    <string name="generationCode" title="Code génération"/>
    <string name="description" title="Désignation"/>
    <string name="argusGenerationReference" title="Réference argus génération"/>
    <boolean name="argusRererenced" title="Référencé argus"/>
  </entity>
</domain-models>